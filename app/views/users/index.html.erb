<% provide :title, 'User index' %>
<%= will_paginate @users, renderer: BootstrapPagination::Rails %>
<ul>
  <% @users.each do |user| %>
    <li>
      <%= link_to user.name, user_path(user) %>  |
      <%= link_to 'edit', edit_user_path(user) %>  | 
      <%= link_to 'delete', user_path(user), method: :delete %>
    </li>
  <% end %>
</ul>
<%= will_paginate @users, renderer: BootstrapPagination::Rails %>
